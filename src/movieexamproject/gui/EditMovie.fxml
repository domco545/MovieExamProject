<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="286.0" prefWidth="410.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="movieexamproject.gui.EditMovieController">
   <children>
      <Label layoutX="48.0" layoutY="24.0" prefHeight="53.0" prefWidth="124.0" text="Edit Movie">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="editMovietxt" layoutX="48.0" layoutY="92.0" promptText="name" />
      <ChoiceBox fx:id="choicebox" layoutX="47.0" layoutY="131.0" prefWidth="150.0" />
      <ListView fx:id="ListView" layoutX="227.0" layoutY="32.0" prefHeight="197.0" prefWidth="150.0" />
      <Button fx:id="RemoveCategoryBtn" layoutX="317.0" layoutY="237.0" mnemonicParsing="false" onAction="#removeCategory" text="Remove" />
      <Button fx:id="addCategoryBtn" layoutX="158.0" layoutY="168.0" mnemonicParsing="false" onAction="#addCategory" text="Add" />
      <Button fx:id="ConfirmBtn" layoutX="34.0" layoutY="237.0" mnemonicParsing="false" onAction="#confirmAndSave" text="Confirm" />
      <Button fx:id="CancelBtn" layoutX="146.0" layoutY="237.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
